+<pre><code>//getter
+var showAnim = $( ".selector" ).datepicker( "option", "showAnim" );
+//setter
+$( ".selector" ).datepicker( "option", "showAnim", 'fold' );</code></pre>
+</dd>
+
+    </dl>
+  </div>
